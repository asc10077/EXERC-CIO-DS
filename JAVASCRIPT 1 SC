    <script>
        function trocaImagem() {
            var elemento=document.getElementById("myImage");
            if (elemento.src.match("bulboff")) {
                elemento.src="https://upload.wikimedia.org/wikipedia/commons/d/df/Sabrina_Carpenter_2019.jpg";
            } else{
                elemento.src="https://upload.wikimedia.org/wikipedia/commons/d/df/Sabrina_Carpenter_2019.jpg";
            }
        }
    </script>
    <p><img id="myImage" onclick="trocaImagem()"
